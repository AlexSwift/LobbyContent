	"Proxies"
	{
		"PlayerProximity"
		{
			"resultVar" "$playerdistance"
			"scale"  1.0
		}	
		"Clamp"
		{
			"srcVar1"	"$playerdistance"
			"min"		"0"
			"max"		"$fadedistance"
