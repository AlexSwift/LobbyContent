			"srcVar2"	"$distanceclamped"
			"resultVar"	"$distanceinverted"
		}
		"PlayerTeamMatch"
		{
			"resultVar"	"$teammatch"
		}
		"Subtract"
		{
			"srcVar1"	"$one"
			"srcVar2"	"$teammatch"
			"resultVar"	"$matchinverted"	
		}
		"Divide"
		{
			"srcVar1"	"$distanceinverted"
			"srcVar2"	"$fadedistance"
			"resultVar" 	"$endalpha"		
		}
		"Multiply"
		{
			"srcVar1"	"$endalpha"		
			"srcVar2"	"$matchinverted"	
			"resultVar" 	"$alpha"	
		}
	}
